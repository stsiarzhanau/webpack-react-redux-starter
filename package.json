{
  "name": "webpack-react-redux-starter",
  "version": "0.7.0",
  "description": "Modern starter kit for React projects.",
  "author": "Aliaksandr Stsiarzhanau <sterzhanoff@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve:src": "cross-env NODE_ENV=development babel-node ./tools/devServer.js",
    "serve:build": "cross-env NODE_ENV=production babel-node ./tools/prodServer.js",
    "prestart": "babel-node ./tools/startMessage.js",
    "start": "npm run serve:src",
    "clean": "rimraf ./build",
    "prebuild": "run-s clean test",
    "build": "cross-env NODE_ENV=production babel-node ./tools/build.js",
    "start:prod": "npm run serve:build",
    "lint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "eslint . --ext .js --ext .jsx --fix ",
    "slint": "stylelint **/*.css",
    "slint:fix": "stylelint **/*.css --fix",
    "test": "cross-env TEST=true mochapack",
    "test:watch": "cross-env TEST=true mochapack --watch",
    "cover": "cross-env COVER=true nyc --reporter=lcov --reporter=text npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index -g"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.css": [
      "stylelint --fix"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@hot-loader/react-dom": "16.13.0",
    "clsx": "1.1.1",
    "connected-react-router": "6.8.0",
    "core-js": "3.6.5",
